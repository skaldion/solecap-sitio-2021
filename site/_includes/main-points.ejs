<section class="main-points-block">
    <div class="container">
        <div class="row">
            <div class="col-sm">
                <img src="<%- image.src %>" alt="<%- image.alt %>">
            </div>
            <div class="col-sm">
                <% mainPoints.forEach((point) => { %>
                    <h3><%- point.heading %></h3>
                    <p><%- point.description %></p>
                <% }); %>
            </div>
        </div>
    </div>
</section>